            server : {
                class : "net.isger.brick.bus.MinaInbound",
                protocol : "command",
                host : "127.0.0.1",
                port : 9000,
                handler : {
                    class : "net.isger.brick.test.bus.TestHandler",
                    prefix : "Server"
                }
            },
            client : {
                class : "net.isger.brick.bus.MinaOutbound",
                protocol : "command",
                host : "127.0.0.1",
                port : 9000,
                handler : {
                    class : "net.isger.brick.test.bus.TestHandler",
                    prefix : "Client"
                }
            }
        }
    },
    /** 调度模块 */
    sched-back : {
        test : {
            interval : "*/5 * * * * ?",
            parameters : {
                name : "employ"
            }
        }
